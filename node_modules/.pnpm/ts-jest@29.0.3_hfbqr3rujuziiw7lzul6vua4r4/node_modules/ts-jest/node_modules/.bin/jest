#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/bigger/Desktop/zhangbin/tal2/mail-be/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/bigger/Desktop/zhangbin/tal2/mail-be/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../jest@29.3.1_zfha7dvnw4nti6zkbsmhmn6xo4/node_modules/jest/bin/jest.js" "$@"
else
  exec node  "$basedir/../../../../../jest@29.3.1_zfha7dvnw4nti6zkbsmhmn6xo4/node_modules/jest/bin/jest.js" "$@"
fi
